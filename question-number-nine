var totalRings = X;
total = 0;

fucntion openRing(a) {
  total = total + 20;
}


function JoinRings(b) {
  total = total + 35;
}

for(var i = 1; i <= totalRings; i++) {
     if((i % 2) == 0) {
        openRing(i);
